{
  "version": "1.0",

  "name": "Postman Monitor Alerts (BitScoop Alexa Demo)",
  "url": "https://api.getpostman.com",
  "headers": {
    "X-Api-Key": "{{ parameters.api_key }}"
  },

  "endpoints": {
    "RunMonitor": {
      "POST": {
        "method": "POST",
        "route": {
          "path": "/monitors/{{ parameters.monitor_id }}/run"
        }
      }
    }
  }
}
